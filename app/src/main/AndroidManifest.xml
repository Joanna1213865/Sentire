<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.songer1993.sentire">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ConnectBT">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
                  android:launchMode="singleTop"/>
        <activity
            android:name=".DesignLibraryVibrationPatterns"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity android:name=".DesignRealtimeVibrationPatterns" />
        <activity android:name=".DesignLightPatterns" />
        <activity android:name=".SeeSavedPatterns" />
        <activity android:name=".Demo" />
        <activity android:name=".MyDeviceList"></activity>
    </application>

</manifest>
